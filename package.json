{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Adattivo Application UI",
  "author": "Adattivo Development Team",
  "private": true,
  "license": "UNLICENSED",
  "main": "webpack.config.js",
  "dependencies": {
    "axios": "^0.21.1",
    "dateformat": "^3.0.3",
    "history": "^4.9.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-addons-linked-state-mixin": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-localization": "^1.0.13",
    "react-redux": "^7.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.2",
    "eslint-plugin-react": "^7.14.2",
    "expect": "^24.8.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "build": "webpack -p",
    "dev-build": "webpack --config webpack.dev.config.js",
    "qa-build": "webpack --config webpack.qa.config.js",
    "lint": "eslint src/.",
    "test": "standard && webpack --bail",
    "dev": "webpack-dev-server --color --config webpack.dev.config.js --content-base src --inline --hot"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "extends": [
      "airbnb",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "semi": 0,
      "comma-dangle": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": 0,
      "react/forbid-prop-types": 0,
      "react/destructuring-assignment": 0,
      "react/no-did-update-set-state": 0,
      "react/no-unknown-property": [2, {"ignore": ["class", "for"]}]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
